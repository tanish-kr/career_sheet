{
  "name": "carrier_sheet",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@reduxjs/toolkit": "^2.8.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/date-fns": "^2.6.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.16",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "@types/react-redux": "^7.1.16",
    "bulma": "^1.0.4",
    "date-fns": "^4.1.0",
    "i18next": "^25.1.2",
    "node-sass": "^9.0.0",
    "react": "19.1.0",
    "react-bulma-components": "4.1.0",
    "react-datepicker": "^8.3.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.15.3",
    "react-i18next": "^15.5.1",
    "react-redux": "^9.2.0",
    "react-scripts": "5.0.1",
    "redux-persist": "^6.0.0",
    "typescript": "~5.8.3"
  },
  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --openssl-legacy-provider build",
    "check-types": "tsc --noEmit",
    "eslint": "eslint src",
    "eslint:fix": "eslint src --fix",
    "format": "prettier --write src",
    "lint": "npm-run-all eslint check-types",
    "lint:fix": "npm-run-all eslint:fix check-types format",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!@codemirror)/'",
    "test:ci": "CI=true npm-run-all test",
    "eject": "react-scripts eject",
    "i18next-extract": "NODE_ENV=development babel './src/**/*.{js,jsx,ts,tsx}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@types/react-datepicker": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^8.32.0",
    "@typescript-eslint/parser": "^8.32.0",
    "babel-plugin-i18next-extract": "^1.0.0",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.3",
    "eslint-plugin-autofix": "^2.2.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "i18next-extract",
        {
          "locales": [
            "ja",
            "en"
          ],
          "outputPath": "./src/locales/{{locale}}.json"
        }
      ]
    ]
  }
}
