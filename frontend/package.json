{
  "name": "carrier_sheet",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-regular-svg-icons": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.20",
    "@types/node": "^16.3.2",
    "@types/react": "18",
    "@types/react-dom": "18",
    "@types/react-redux": "^7.1.16",
    "bulma": "^0.9.2",
    "i18next": "^20.3.3",
    "node-sass": "^8",
    "react": "18",
    "react-bulma-components": "4.0.7",
    "react-datepicker": "^4.3.0",
    "react-dom": "18",
    "react-hook-form": "^7.15.3",
    "react-i18next": "^11.8.5",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.3",
    "redux-persist": "^6.0.0",
    "typescript": "~4.3.5"
  },
  "scripts": {
    "start": "react-scripts --openssl-legacy-provider start",
    "build": "react-scripts --openssl-legacy-provider build",
    "check-types": "tsc --noEmit",
    "eslint": "eslint src",
    "eslint:fix": "eslint src --fix",
    "format": "prettier --write src",
    "lint": "npm-run-all eslint check-types",
    "lint:fix": "npm-run-all eslint:fix check-types format",
    "test": "react-scripts test --transformIgnorePatterns 'node_modules/(?!@codemirror)/'",
    "test:ci": "CI=true npm-run-all test",
    "eject": "react-scripts eject",
    "i18next-extract": "NODE_ENV=development babel './src/**/*.{js,jsx,ts,tsx}'"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@types/react-datepicker": "^4.1.7",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "babel-plugin-i18next-extract": "^0.8.2",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-autofix": "^1.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "plugins": [
      [
        "i18next-extract",
        {
          "locales": [
            "ja",
            "en"
          ],
          "outputPath": "./src/locales/{{locale}}.json"
        }
      ]
    ]
  }
}
